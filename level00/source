#include <stdio.h>

int main()
{
   puts("***********************************");
   puts(" * \t - Level00 -\t\t * ");
   puts("***********************************");

   printf("Password:");
   int a;
   scanf("%d", &a);

   if (a == 5276)
   {
      puts("\nAuthenticated!");
      system("/bin/sh");
   }
   else
      puts("\nInvalid Password!");
   return 0;
}